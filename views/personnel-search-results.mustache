<h2>_("Search results")</h2>

{{#personnel_status}}
  <table class="table table-striped table-condensed">
    <tr>
      <th>_("Name")</th>
      <th>_("E-mail")</th>
      <th>_("Municipality")</th>
      <th>_("Phone")</th>
      <th>_("Title")</th>
      <th>_("Qualities")</th>
      <th>_("Responsibilities")</th>
    </tr>
    {{#people}}
      <tr class="{{qualities}}">
        <td>{{first_name}} {{last_name}}</td>
        <td>{{contact.email}}</td>
        <td>{{organisation.contact.street_address.municipality__("locale")}}</td>
        <td>{{contact.telephone}}</td>
        <td>{{job_title__("locale")}}</td>
        <td>{{qualities}}</td>
        <td>{{responsibility__("locale")}}</td>
      </tr>
    {{/people}}
  </table>
{{/personnel_status}}

{{^personnel_status}}
  <div class="alert alert-error span2"><strong>_("No results found")</strong></div>
{{/personnel_status}}
