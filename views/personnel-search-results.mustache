<h2>Hakutulokset</h2>

{{#personnel_status}}
  <table class="table table-striped table-condensed">
    <tr>
      <th>Nimi</th>
      <th>Sähköposti</th>
      <th>Kunta/Toimipiste</th>
      <th>Puhelin</th>
      <th>Titteli</th>
      <th>Osaamisalue</th>
      <th>Vastuualueet</th>
    </tr>
    {{#people}}
      <tr class="{{qualities}}">
        <td>{{first_name}} {{last_name}}</td>
        <td>{{contact.email}}</td>
        <td>{{organisation.contact.street_address.municipality_fi}}</td>
        <td>{{contact.telephone}}</td>
        <td>{{job_title_fi}}</td>
        <td>
          {{#qualities}}
            {{.}},
          {{/qualities}}
          
        </td>
        <td>{{responsibility_fi}}</td>
      </tr>
    {{/people}}
  </table>
{{/personnel_status}}

{{^personnel_status}}
  <div class="alert alert-error span2"><strong>Ei hakutuloksia</strong></div>
{{/personnel_status}}
