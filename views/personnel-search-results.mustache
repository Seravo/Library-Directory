<h2>Hakutulokset</h2>

{{#personnel_status}}
  <table class="table table-striped table-condensed">
    <tr><th>Nimi<th>Sähköposti<th>Puhelin<th>Titteli<th>Vastuualueet
    {{#people}}
      <tr><td>{{last_name}}, {{first_name}}<td>{{contact.email}}<td>{{contact.telephone}}<td>{{job_title_fi}}<td>{{responsibility_fi}}
    {{/people}}
  </table>
{{/personnel_status}}

{{^personnel_status}}
  <div class="alert alert-error span2"><strong>Ei hakutuloksia</strong></div>
{{/personnel_status}}
