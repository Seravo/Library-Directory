<h2>_("Search results")</h2>

{{#personnel_status}}
  <table class="table table-striped table-condensed">
    <tr>
      <th>_("Name") Nimi</th>
      <th>_("E-mail") Sähköposti</th>
      <th>_("Municipality") Kunta/Toimipiste</th>
      <th>_("Phone") Puhelin</th>
      <th>_("Title") Titteli</th>
      <th>_("Qualities") Osaamisalue</th>
      <th>_("Responsibilities") Vastuualueet</th>
    </tr>
    {{#people}}
      <tr class="{{qualities}}">
        <td>{{first_name}} {{last_name}}</td>
        <td>{{contact.email}}</td>
        <td>{{organisation.contact.street_address.municipality_fi}}</td>
        <td>{{contact.telephone}}</td>
        <td>{{job_title_fi}}</td>
        <td>
          {{#qualities}}
            {{.}},
          {{/qualities}}
          
        </td>
        <td>{{responsibility_fi}}</td>
      </tr>
    {{/people}}
  </table>
{{/personnel_status}}

{{^personnel_status}}
  <div class="alert alert-error span2"><strong>_("No results found")</strong></div>
{{/personnel_status}}
